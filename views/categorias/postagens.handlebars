{{#each postagens}}
  <div class="card mt-4">
    <div class="card-body">
      <h3>{{titulo}}</h3>
      <p>{{descricao}}</p>
      <a href="/postagem/{{slug}}"><button class="btn-sm btn-success">Leia mais</button></a>
      <hr>
      <small>Data de publicação: {{_creationDate}}</small>
    </div>
  </div>
{{else}}
  <h5 class="mt-3"> Nenhuma postagem</h5>
{{/each}}
